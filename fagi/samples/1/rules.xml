<rules>
	<defaultDatasetAction>keep-right</defaultDatasetAction>	
	<rule>
		<propertyA>label</propertyA>
		<propertyB>label</propertyB>
		<externalProperty id="a1">http://www.w3.org/2000/01/rdf-schema#label</externalProperty>
		<externalProperty id="b1">http://www.w3.org/2000/01/rdf-schema#label</externalProperty>
		<externalProperty id="a2">http://linkedgeodata.org/ontology/cuisine</externalProperty>
		<externalProperty id="b2">http://linkedgeodata.org/ontology/cuisine</externalProperty>		
		<actionRuleSet>
			<actionRule>
				<condition>
					<function>isLiteralAbbreviation(a2)</function>
				</condition>
				<action>keep-right</action>
			</actionRule>
		</actionRuleSet>
		<defaultAction>keep-left</defaultAction>
	</rule>	
	<rule>
		<propertyA>wkt</propertyA>
		<propertyB>wkt</propertyB>
		<defaultAction>keep-right</defaultAction>
	</rule>		
	<rule>
		<propertyA>http://purl.org/dc/terms/modified</propertyA>
		<propertyB>http://purl.org/dc/terms/modified</propertyB>
		<externalProperty id="a1">phone</externalProperty>
		<externalProperty id="b1">phone</externalProperty>
		<actionRuleSet>		
			<actionRule>
				<condition>
					<expression>
						<not>
							<function>isDateKnownFormat(a)</function>
						</not>
					</expression>
				</condition>
				<action>keep-left</action>
			</actionRule>
		</actionRuleSet>
		<defaultAction>keep-right</defaultAction>
	</rule>
</rules>
